<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
 <extension-point id="Textprocessor" name="Textprozessor" schema="schema/Textprocessor.exsd"/>
   <extension
         point="ch.elexis.core.ui.Text">
      <Texterweiterung
            Klasse="ch.elexis.ungrad.text.templator.ui.OdfTextPlugin"
            name="Ungrad Text-Templator">
      </Texterweiterung>
   </extension>
   <extension
         point="ch.elexis.ungrad.text.templator.Textprocessor">
      <Textprocessor
            Name="OpenOffice-Prozessor"
            clazz="ch.elexis.ungrad.text.templator.model.OpenOfficeProcessor"
            name="OpenOffice-Processor">
      </Textprocessor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="ch.elexis.preferences.Texterstellung"
            class="ch.elexis.ungrad.text.templator.ui.Preferences"
            id="ch.elexis.ungrad.text.templator.Preferences"
            name="Ungrad-TextTemplator">
      </page>
      <page
            category="ch.elexis.ungrad.text.templator.Preferences"
            class="ch.elexis.ungrad.text.templator.ui.OOOProcessorPrefs"
            id="ch.elexis.ungrad.text.templator.oooprocessor.preferences"
            name="OpenOffice.org Processor">
      </page>
   </extension>

</plugin>
